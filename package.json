{
  "name": "shopping-cart-app",
  "version": "0.1.0",
  "description": "A React shopping cart application with Redux Toolkit",
  "scripts": {
    "start": "better-npm-run start",
    "prebuild": "npm run clean && npm test",
    "build": "better-npm-run prod",
    "lint": "eslint .",
    "format": "prettier --write .",
    "stats": "better-npm-run prodStats",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist build"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --mode development --config ./webpack/dev.config.js --open",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "webpack --config ./webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "prodStats": {
      "command": "if not exist dist mkdir dist && webpack --config ./webpack/prod.config.js --json > dist/prodStats.json",
      "env": {
        "NODE_ENV": "production",
        "ANALYZE": true
      }
    }
  },
  "author": "Ajit Tupe",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "rc-slider": "^11.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.20",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.26.6",
    "@babel/plugin-transform-optional-chaining": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.5",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "better-npm-run": "^0.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.41.0",
    "css-loader": "^6.7.0",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-webpack-plugin": "^5.0.0",
    "file-loader": "^6.2.0",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "mini-css-extract-plugin": "^2.6.0",
    "path-browserify": "^1.0.1",
    "postcss-loader": "^8.1.1",
    "prettier": "3.5.3",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.6.0",
    "stream-browserify": "^3.0.0",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^6.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
